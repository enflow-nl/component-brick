{
    "name": "enflow/component-brick",
    "description": "Brick package that will add utils to make it easier to work together with the phonegap implemenation.",
    "authors": [
        {
            "name": "Michel Bardelmeijer",
            "email": "michel@enflow.nl"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.enflow.nl/"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.4",
        "illuminate/routing": "^5.4",
        "illuminate/database": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Enflow\\Component\\Brick\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Enflow\\Component\\Brick\\ServiceProvider"
            ]
        }
    }
}
